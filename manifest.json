{
  "name": "Bruin Score Check",
  "version": "0.1.1",
  "description": "Calculates your total unweighted grade on MyUcla",
  "content_scripts": [
    {
      "matches": ["*://be.my.ucla.edu/IWE/mygrades.aspx"],
      "js": ["summate.js"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "images/bruinicon16.png",
    "32": "images/bruinicon32.png",
    "64": "images/bruinicon64.png",
    "128": "images/bruinicon128.png"
  },
  "manifest_version": 2
}
